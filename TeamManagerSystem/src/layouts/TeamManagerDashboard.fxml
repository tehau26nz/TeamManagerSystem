	                  <Button fx:id="btnPlayers" mnemonicParsing="false" onMouseClicked="#handleTeamManagerDashboardButtonAction" prefHeight="50.0" prefWidth="200.0" text="Manage Players">
	                     <font>
	                        <Font name="Gill Sans MT Bold" size="13.0" />
	                     </font>
	                  </Button>
	                    <Button id="btnPlayers" fx:id="btnEvents" mnemonicParsing="false" onMouseClicked="#handleTeamManagerDashboardButtonAction" prefHeight="50.0" prefWidth="200.0" text="View Events">
